package tryMethods;

class Person {
	
	String name;
	int age;
	
}

public class application {

	public static void main(String[] args) {
		Person person1 = new Person();
		person1.name = "daku";
		
		Person person2 = new Person();
		person2.name = "koko";
		
	  System.out.println(person1.name);
	
	}
	
        
	
}
