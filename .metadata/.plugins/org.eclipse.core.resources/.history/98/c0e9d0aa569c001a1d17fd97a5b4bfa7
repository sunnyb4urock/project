package stotic;

class thing {
	public String name;
	public static String description;
}

public class app {

	public static void main(String[] args) {

		thing thing1 = new thing();
		thing thing2= new thing ();
		thing1.name = "bob";
		thing2.name = "the builder";
		
		System.out.println(thing1.name);
		System.out.println(thing2.name);
	}

}
