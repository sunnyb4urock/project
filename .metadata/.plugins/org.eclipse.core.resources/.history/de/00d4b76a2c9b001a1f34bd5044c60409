package tryMethods;

class Person {
	
	String name;
	int age;
	
	void speak() {
		System.out.println("My name is " + name + " i am "+ age +" yesrs of age");
	}
	
}

public class application {

	public static void main(String[] args) {
		Person person1 = new Person();
		person1.name = "daku";
		person1.speak();
		
		Person person2 = new Person();
		person2.name = "koko";
		
	  System.out.println(person1.name);
	
	}
	
        
	
}
