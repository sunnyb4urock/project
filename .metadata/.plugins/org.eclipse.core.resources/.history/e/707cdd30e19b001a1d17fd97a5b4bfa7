package setting;

class frog {
	int age ;
	String name;
	
	public String getName() {
		return name;
		
	}
	public int getAge() {
		return age;
	}
}

public class application {

	public static void main(String[] args) {
		frog frog1= new frog();
		  frog1.name = "Bertie";
		  frog1.age= 1 ;
		  
		  System.out.println(frog1.getName());
		  
	}

}
