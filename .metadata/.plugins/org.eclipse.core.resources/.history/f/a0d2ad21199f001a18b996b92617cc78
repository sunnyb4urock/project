package generaics;

import java.util.ArrayList;

public class App {

	public static void main(String[] args) {
		
		////Before java 5///////
		
		ArrayList list = new ArrayList ();
		
		list.add("apple");
		list.add("orange");
		list.add("banana");
		
		String fruit =  (String)list.get(1);
		
		System.out.println(fruit);
		 
	}

}
